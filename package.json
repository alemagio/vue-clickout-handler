{
  "name": "vue-clickout-handler",
  "repository": "alemagio/vue-clickout-handler",
  "version": "1.0.0",
  "description": "A simple zero dependencies directive to detect click events outside of a certain element",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "test": "c8 --all ava"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "c8": "^7.3.0"
  },
  "ava": {
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "files": [
    "lib"
  ],
  "c8": {
    "include": "lib"
  }
}
